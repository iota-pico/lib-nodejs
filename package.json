{
    "name": "@iota-pico/lib-nodejs",
    "version": "0.9.0",
    "description": "IOTA Pico Framework Library for NodeJS",
    "main": "pkg/iota-pico-lib-nodejs.js",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "keywords": [
        "iota",
        "pico",
        "interface",
        "tangle",
        "javascript",
        "typescript",
        "API"
    ],
    "directories": {
        "doc": "docs",
        "test": "test"
    },
    "author": "martyn@iota.eco",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/iotaeco/iota-pico-lib-nodejs.git"
    },
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "build-clean": "rimraf ./dist/*",
        "build-compile": "tsc",
        "build": "npm-run-all build-clean build-compile",
        "pkg-clean": "rimraf ./pkg/*",
        "pkg-dev": "cross-env NODE_ENV=development webpack --progress",
        "pkg-prod": "cross-env NODE_ENV=production webpack --progress",
        "pkg-dts": "node ./dist/build-dts.js",
        "pkg-finalise": "rimraf ./pkg/bootstrap.js",
        "pkg": "npm-run-all pkg-clean pkg-dev pkg-prod pkg-dts pkg-finalise",
        "dist": "npm-run-all build pkg"
    },
    "dependencies": {
        "big-integer": "^1.6.26",
        "ffi": "^2.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.40",
        "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
        "@babel/preset-env": "^7.0.0-beta.40",
        "@babel/runtime": "^7.0.0-beta.40",
        "@iota-pico/api": "^0.9.0",
        "@iota-pico/business": "^0.9.0",
        "@iota-pico/core": "^0.9.0",
        "@iota-pico/crypto": "^0.9.0",
        "@iota-pico/data": "^0.9.0",
        "@iota-pico/pal-nodejs": "^0.9.0",
        "@iota-pico/pow-box": "^0.9.0",
        "@iota-pico/pow-js": "^0.9.0",
        "@iota-pico/pow-nodejs": "^0.9.0",
        "@iota-pico/pow-wasm": "^0.9.0",
        "@types/chai": "^4.1.1",
        "@types/mocha": "^2.2.46",
        "babel-loader": "^8.0.0-beta.0",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "chai": "^4.1.2",
        "copy": "^0.3.1",
        "coveralls": "^3.0.0",
        "cross-env": "^5.1.3",
        "minicat": "^1.0.0",
        "mocha": "^5.0.0",
        "npm-run-all": "^4.1.2",
        "nsp": "^3.1.0",
        "nyc": "^11.4.1",
        "rimraf": "^2.6.2",
        "source-map-support": "^0.5.3",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "tslint-microsoft-contrib": "^5.0.2",
        "typedoc": "^0.10.0",
        "typedoc-plugin-markdown": "^1.0.12",
        "typescript": "^2.7.1",
        "uglifyjs-webpack-plugin": "^1.2.3",
        "webpack": "^3.11.0",
        "webpack-cli": "^2.0.10"
    }
}
